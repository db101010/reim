        <description/>
        <name>apApprovalButton</name>
        <script><![CDATA[function apApprovalButton(){
		var ourUser = gs.getUserID();
	    var canApp = false;
		if(ourUser == current.approver) {
			canApp = true;
		}
		
		if (current.state == '1' && (gs.hasRole('admin') || canApp == true)) {
			return true;
		} else {
			return false;
		}
}]]></script>
